  echo 'ls -l'
  echo 'sudo rmdir testdir'
  echo 'ls -l'
[912-END]

[913]
NAME="Copying and Removing File"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'touch myfile'
  echo 'ls -l'
  echo 'cp myfile myfile_copy'
  echo 'ls -l'
  echo 'rm myfile_copy'
  echo 'ls -l'
  echo 'rm myfile'
  echo 'ls -l'
[913-END]

