[937]
NAME="ls with pipe and redirection"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'ls -l | cat -e > .lst'
  echo 'cat .lst'
  echo 'rm .lst'
[937-END]

