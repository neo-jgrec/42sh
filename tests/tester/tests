[926]
NAME="cd with relative path"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'cd ..'
  echo 'ls -l'
  echo 'cd /usr'
  echo 'ls -l'
  echo 'cd -'
  echo 'ls -l'
  echo 'cd -'
  echo 'ls -l'
  echo 'pwd'
  echo 'cd /'
  echo 'pwd'
  echo 'cd /usr'
[926-END]

[927]
